@model NodeCreateViewModel

@{
}
<h1>Hello QW</h1>
<form method="post" action="Create">
    <div class="form-group">
        <label for="">Node Name</label>
        <input type="text" class="form-control" asp-for="NodeViewModel.NodeName" placeholder="Enter Node Name">

    </div>
    <div class="form-group col-md-4">
        <label for="inputState">State</label>
        <select id="inputState" class="form-control" asp-for="NodeViewModel.ParentNodeId" >
            <option>select</option>
            @foreach(var item in Model.NodeViews.ToList())
            {
                <option value="@item.NodeId">@item.NodeName</option>
            }
        </select>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>